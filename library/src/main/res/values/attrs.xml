<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="DatePicker">
    <attr name="date_picker_selection_vibrates" format="boolean" />
    <attr name="date_picker_header_background_color" format="color" />
    <attr name="date_picker_background_color" format="color" />
    <attr name="date_picker_background_radius" format="dimension" />
    <attr name="date_picker_selection_color" format="color" />
    <attr name="date_picker_normal_font" format="reference" />
    <attr name="date_picker_medium_font" format="reference" />
    <attr name="date_picker_calendar_horizontal_padding" format="dimension" />
    <attr name="date_picker_calendar_today_stroke_color" format="color" />
    <attr name="date_picker_title" format="string" />
    <attr name="date_picker_manual_input_label" format="string" />
    <attr name="date_picker_default_mode" format="enum">
      <enum name="calendar" value="1" />
      <enum name="year_list" value="2" />
      <enum name="input_edit" value="3" />
    </attr>
  </declare-styleable>
</resources>